<!DOCTYPE flight_plan SYSTEM "flight_plan.dtd">

<flight_plan alt="230" ground_alt="207" lat0="50.788812" lon0="6.046862" max_dist_from_home="170" name="Imav2015 Carto" security_height="2">
  <header>
#include "autopilot.h"
</header>
  <waypoints>
    <waypoint name="HOME" x="0.0" y="0.0"/>
    <waypoint name="CLIMB" x="16.9" y="-12.5"/>
    <waypoint name="STDBY" x="13.4" y="-5.2"/>
    <waypoint name="TD" x="9.5" y="-13.8"/>
    <waypoint name="p1" x="14.0" y="-2.3"/>
    <waypoint name="p2" x="-37.6" y="41.0"/>
    <waypoint name="p3" x="-43.6" y="33.9"/>
    <waypoint name="p4" x="9.1" y="-7.8"/>
    <waypoint name="p5" x="4.0" y="-14.0"/>
    <waypoint name="p6" x="-49.5" y="27.0"/>
    <waypoint name="p7" x="-55.8" y="20.4"/>
    <waypoint name="p8" x="-1.1" y="-20.6"/>
    <waypoint name="_FLY1" x="1.6" y="-37.0"/>
    <waypoint name="_FLY2" x="32.6" y="0.6"/>
    <waypoint name="_FLY3" x="14.2" y="16.9"/>
    <waypoint name="_FLY4" x="77.2" y="95.4"/>
    <waypoint name="_FLY5" x="13.3" y="147.6"/>
    <waypoint name="_FLY6" x="-100.5" y="30.1"/>
    <waypoint name="_KILL1" x="1.8" y="-42.1"/>
    <waypoint name="_KILL2" x="36.4" y="2.9"/>
    <waypoint name="_KILL3" x="90.0" y="100.0"/>
    <waypoint name="_KILL4" x="12.6" y="159.9"/>
    <waypoint name="_KILL5" x="-109.7" y="29.8"/>
  </waypoints>
  <sectors>
    <sector color="lime" name="Flight_Area">
      <corner name="_FLY1"/>
      <corner name="_FLY2"/>
      <corner name="_FLY3"/>
      <corner name="_FLY4"/>
      <corner name="_FLY5"/>
      <corner name="_FLY6"/>
    </sector>
    <sector color="red" name="Kill">
      <corner name="_KILL1"/>
      <corner name="_KILL2"/>
      <corner name="_KILL3"/>
      <corner name="_KILL4"/>
      <corner name="_KILL5"/>
    </sector>
  </sectors>
  <blocks>
    <block name="Wait GPS">
      <call fun="NavKillThrottle()"/>
      <while cond="!GpsFixValid() && gps.pacc > 1500"/>
    </block>
    <block name="Geo init">
      <while cond="LessThan(NavBlockTime(), 10)"/>
      <call fun="NavSetAltitudeReferenceHere()"/>
    </block>
    <block name="Holding point">
      <call fun="NavKillThrottle()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block group="home" name="Start Engine" strip_button="Start Engine" strip_icon="on.png">
      <call fun="NavResurrect()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block group="home" name="Takeoff" strip_button="Takeoff" strip_icon="takeoff.png">
      <exception cond="stateGetPositionEnu_f()->z > 2.0" deroute="Start Mission"/>
      <call fun="NavSetWaypointHere(WP_CLIMB)"/>
      <stay climb="0.5" vmode="climb" wp="CLIMB"/>
    </block>
    <block group="home" name="Standby" pre_call="if(!InsideKill(GetPosX(), GetPosY())) NavKillThrottle();" strip_button="Standby" strip_icon="home.png">
      <stay wp="STDBY"/>
    </block>
    <block name="Start Mission">
      <stay until="stage_time>30" wp="STDBY"/>
      <go from="STDBY" hmode="route" wp="p1"/>
    </block>
    <block name="Mapping" strip_button="Carto" strip_icon="survey.png">
      <set value="OSD_CMD_CAPTURE_RUN" var="osd_cmd"/>
      <path wpts="p1,p2,p3,p4,p5,p6,p7,p8"/>
      <set value="OSD_CMD_CAPTURE_STOP" var="osd_cmd"/>
      <go from="p8" hmode="route" wp="STDBY"/>
    </block>
    <block name="Land from STDBY">
      <go from="STDBY" hmode="route" wp="TD"/>
      <deroute block="land"/>
    </block>
    <block name="land here">
      <call fun="NavSetWaypointHere(WP_TD)"/>
    </block>
    <block name="land" strip_button="Land" strip_icon="land-right.png">
      <go wp="TD"/>
    </block>
    <block name="flare">
      <exception cond="NavDetectGround()" deroute="Holding point"/>
      <exception cond="!nav_is_in_flight()" deroute="landed"/>
      <call fun="NavStartDetectGround()"/>
      <stay climb="-0.8" vmode="climb" wp="TD"/>
    </block>
    <block name="landed">
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
  </blocks>
</flight_plan>
